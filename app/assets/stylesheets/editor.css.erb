/* STATIC CONTROL BAR STYLES*/
#staticControl {
	position:fixed;
	top:50px;
	right:0px;
}

#staticControl button {
	display:inline-block;
	float:right;
	clear:right;
}

/*Text Button Styles */
.ui-button-text-only .ui-button-text {
    padding: 0.1em 0.3em 0em 0.3em;
    height:30px;
    overflow:hidden;
}